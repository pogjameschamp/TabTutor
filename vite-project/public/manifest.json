{
    "manifest_version": 3,
    "name": "TabTutor",
    "version": "1.0",
    "action": {
      "default_popup": "index.html",
      "default_icon": "vite.svg"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "icons": {
      "48": "vite.svg"
    },
    "permissions": [
      "activeTab"
    ],
    "background": {
      "service_worker": "background.js"
    }
  }
  